h1,
h2,
h3,
h4,
h5 {
	margin: 0;
}

h1 {
  font-weight: 400
  font-size: 32px
  line-height: 42px
}

h2 {
	font-weight: 400
	font-size: 24px
}

h3 {
	font-weight: bold
	font-size: 26px
	line-height: 1.45em
}

h4 {
	font-weight: bold
	font-size: 18px
}

span {
	font-weight: 400
	font-size: 14px
	line-height: 16px
	color: rgb(153, 153, 153)
	text-transform: uppercase
}

.overline {
	font-size: 12px
	line-height: 16px
	letter-spacing: 2px
	text-transform: uppercase
}

a {
	color: var(--color-black)
}